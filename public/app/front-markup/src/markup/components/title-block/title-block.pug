mixin title-block(data)
    +b.title-block()&attributes(attributes)
        block
        +e.H1.title #{data.title}
        +e.SPAN.subtitle
            != pugHelpers.Icon.call(locals, {iconName: 'point', className: 'title-block__ico title-block__ico_point', iconWidth: '0', iconHeight: '0'})
            | #{data.subtitle}
